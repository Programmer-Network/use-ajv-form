{
  "name": "use-ajv-form",
  "version": "1.0.0",
  "description": "Custom React Hook that integrates with Ajv JSON Schema Validator",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agjs/use-ajv-form.git"
  },
  "author": "Aleksandar Grbic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agjs/use-ajv-form/issues"
  },
  "homepage": "https://github.com/agjs/use-ajv-form#readme",
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.3",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ]
}
